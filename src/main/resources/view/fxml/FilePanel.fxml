<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefHeight="400.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.name.lasertriangulation.Controllers.FilePanel">
         <children>
            <ListView fx:id="listView" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="220.0" VBox.vgrow="ALWAYS" onKeyReleased="#onListClicked" onMouseClicked="#onListClicked">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </ListView>
            <ButtonBar buttonMinWidth="80.0" prefHeight="40.0" prefWidth="200.0" VBox.vgrow="NEVER">
              <buttons>
                <Button fx:id="load" mnemonicParsing="false" onAction="#onClickAdd" text="Добавить" />
                  <Button fx:id="delete" disable="true" mnemonicParsing="false" onAction="#onClickDelete" text="Удалить" />
              </buttons>
         <VBox.margin>
            <Insets bottom="10.0" right="15.0" />
         </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
